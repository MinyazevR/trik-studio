<constraints>
    <!-- Временное ограничение -->
    <timelimit value="150000"/>
    <!-- Инициализация переменных -->
    <init>
        <!-- Итоговые баллы за полигон -->
        <setter name="score">
            <int value="0"/>
        </setter>
        <!-- Баллы за выезд из зоны старта -->
        <setter name="start_score">
            <int value="2"/>
        </setter>
        <!-- Баллы за смещение кегли -->
        <setter name="move_pin_score">
            <int value="1"/>
        </setter>
        <!-- Баллы за нахождение кегли полностью в зоне -->
        <setter name="pin_full_score">
            <int value="1"/>
        </setter>
        <!-- Штраф за смещение ненужной кегли -->
        <setter name="pin_penalty_score">
            <int value="2"/>
        </setter>
        <!-- Баллы за остановку в зоне старта -->
        <setter name="finish_score">
            <int value="1"/>
        </setter>
        <!-- Наличие маркера в зоне 1 -->
        <setter name="sample_1">
            <int value="1"/>
        </setter>
        <!-- Наличие маркера в зоне 2 -->
        <setter name="sample_2">
            <int value="1"/>
        </setter>
        <!-- Наличие маркера в зоне 3 -->
        <setter name="sample_3">
            <int value="1"/>
        </setter>
        <!-- Наличие маркера в зоне 4 -->
        <setter name="sample_4">
            <int value="1"/>
        </setter>
        <!-- Наличие маркера в зоне 5 -->
        <setter name="sample_5">
            <int value="1"/>
        </setter>
        <!-- Наличие маркера в зоне 6 -->
        <setter name="sample_6">
            <int value="1"/>
        </setter>
        <!-- Наличие маркера в зоне 7 -->
        <setter name="sample_7">
            <int value="1"/>
        </setter>
        <!-- Наличие маркера в зоне 8 -->
        <setter name="sample_8">
            <int value="1"/>
        </setter>
    </init>
    <!-- Описание событий -->
    <!-- Проверка покидания роботом зоны старта -->
    <event settedUpInitially="true" id="out-from-start-checker">
        <condition>
            <not>
                <inside regionId="start_zone" objectPoint="any" objectId="robot1"/>
            </not>
        </condition>
        <triggers>
            <setter name="score">
                <sum>
                    <variableValue name="score"/>
                    <variableValue name="start_score"/>
                </sum>
            </setter>
            <message text="Робот покинул зону старта, начислены баллы за старт!"/>
            <message text="Набрано баллов: %text1%">
                <replace var="text1">
                    <variableValue name="score"/>
                </replace>
            </message>
            <setUp id="pin-move-01-checker"/>
            <setUp id="pin-move-02-checker"/>
            <setUp id="pin-move-03-checker"/>
            <setUp id="pin-move-04-checker"/>
            <setUp id="pin-move-05-checker"/>
            <setUp id="pin-move-06-checker"/>
            <setUp id="pin-move-07-checker"/>
            <setUp id="pin-move-08-checker"/>
            <setUp id="pin-penalty-01-checker"/>
            <setUp id="pin-penalty-02-checker"/>
            <setUp id="pin-penalty-03-checker"/>
            <setUp id="pin-penalty-04-checker"/>
            <setUp id="pin-penalty-05-checker"/>
            <setUp id="pin-penalty-06-checker"/>
            <setUp id="pin-penalty-07-checker"/>
            <setUp id="pin-penalty-08-checker"/>
            <success deferred="true"/>
        </triggers>
    </event>
    <!-- КЕГЛЯ 1 -->
    <!-- Проверка смещения кегли 1 -->
    <event settedUpInitially="false" id="pin-move-01-checker">
        <conditions glue="and">
            <notEqual>
                <variableValue name="sample_1"/>
                <int value="0"/>
            </notEqual>
            <not>
                <inside regionId="start_pin_zone_1" objectPoint="any" objectId="{pin_01}"/>
            </not>
        </conditions>
        <triggers>
            <setter name="score">
                <sum>
                    <variableValue name="score"/>
                    <variableValue name="move_pin_score"/>
                </sum>
            </setter>
            <message text="Начислены баллы за смещение кегли 1."/>
            <message text="Набрано баллов: %text1%">
                <replace var="text1">
                    <variableValue name="score"/>
                </replace>
            </message>
            <setUp id="pin-full-01-checker"/>
            <setUp id="return-to-start-checker"/>
        </triggers>
    </event>
    <!-- Проверка штрафа за смещение кегли 1 -->
    <event settedUpInitially="false" id="pin-penalty-01-checker">
        <conditions glue="and">
            <equals>
                <variableValue name="sample_{id}"/>
                <int value="0"/>
            </equals>
            <not>
                <inside regionId="start_pin_zone_{id}" objectPoint="any" objectId="{pin_0{id}}"/>
            </not>
        </conditions>
        <triggers>
            <setter name="score">
                <difference>
                    <variableValue name="score"/>
                    <variableValue name="pin_penalty_score"/>
                </difference>
            </setter>
            <message text="Начислен штраф за неверное смещение кегли {id}."/>
            <message text="Набрано баллов: %text1%">
                <replace var="text1">
                    <variableValue name="score"/>
                </replace>
            </message>
        </triggers>
    </event>
    <!-- Проверка полного попадания кегли 1 в зону -->
    <event settedUpInitially="false" id="pin-full-01-checker">
        <condition>
            <inside regionId="green_zone_1" objectPoint="all" objectId="{pin_01}"/>
        </condition>
        <triggers>
            <setter name="score">
                <sum>
                    <variableValue name="score"/>
